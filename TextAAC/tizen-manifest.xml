<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<manifest xmlns="http://tizen.org/ns/packages" api-version="2.3.1" package="org.example.systemsettings2" version="1.0.2">
    <profile name="wearable"/>
    <ui-application appid="org.example.systemsettings2" exec="systemsettings2" multiple="false" nodisplay="false" taskmanage="true" type="capp">
        <label>TextAAC</label>
        <icon>logo_character_img.png</icon>
    </ui-application>
    <privileges>
        <privilege>http://tizen.org/privilege/appmanager.launch</privilege>
        <privilege>http://tizen.org/privilege/audiorecorder</privilege>
        <privilege>http://tizen.org/privilege/recorder</privilege>
        <privilege>http://tizen.org/privilege/userprofile.read</privilege>
    </privileges>
    <feature name="http://tizen.org/feature/speech.recognition">true</feature>
    <feature name="http://tizen.org/feature/speech.synthesis">true</feature>
    <feature name="http://tizen.org/feature/microphone">true</feature>
    <feature name="http://tizen.org/feature/speech.control">true</feature>
</manifest>
